
config 'global' 'automount'
	option 'from_fstab' '1'
	option 'anon_mount' '1'

config 'global' 'autoswap'
	option 'from_fstab' '1'
	option 'anon_swap' '0'

config 'mount'
	option 'target' '/mnt/usbdisk'
	option 'device' '/dev/sda1'
	option 'options' 'rw,sync'
	option 'enabled_fsck' '0'
	option 'enabled' '1'
	option 'fstype' 'auto'

config 'swap'
	option 'device' '/dev/sda2'
	option 'enabled' '0'

