
config 'wifi-device' 'radio0'
	option 'type' 'mac80211'
option 'channel' '6'
	option 'macaddr' '00:25:9c:22:5b:b4'
	option 'hwmode' '11g'
	option 'htmode' 'HT20'
	list 'ht_capab' 'SHORT-GI-40'
	list 'ht_capab' 'DSSS_CCK-40'

config 'wifi-iface'
	option 'device' 'radio0'
	option 'network' 'lan'
	option 'mode' 'ap'
option 'encryption' 'psk'
	option 'key' 'myP@5sw0rd'
option 'ssid' '8netdev'

